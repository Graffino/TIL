GOOGLE_CLIENT_ID=__GOOGLE_CLIENT_ID__
GOOGLE_CLIENT_SECRET=__GOOGLE_CLIENT_SECRET__
GOOGLE_REDIRECT_URL=__GOOGLE_REDIRECT__CALLBACK__
GOOGLE_DOMAIN=__GOOGLE_DOMAIN__

SLACK_POST_ENDPOINT=__SLACK_POST_ENDPOINT__
MEDIUM_TOKEN=__MEDIUM_TOKEN__
DEFAULT_TWITTER_HANDLE=__TWITTER_HANDLE__
TWITTER_ACCESS_TOKEN=__TWITTER_ACCESS_TOKEN__
TWITTER_ACCESS_SECRET=__TWITTER_ACCESS_SECRET__
