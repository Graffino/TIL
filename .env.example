APP_NAME=TIL
APP_ENV=local
APP_KEY=__APP_KEY__
APP_DEBUG=true
APP_LOG_LEVEL=debug
APP_URL=__APP_URL__
APP_URL_REWRITE=${APP_URL}
DB_CONNECTION=pgsql
DB_HOST=__DB_HOST__
DB_PORT=__DB_PORT__
DB_DATABASE=__DB_NAME__
DB_USERNAME=__DB_USERNAME__
DB_PASSWORD=__DB_PASSWORD__

BROADCAST_DRIVER=pusher
CACHE_DRIVER=file
SESSION_DRIVER=file
SESSION_LIFETIME=120
